{
	"name": "jass-plugin",
	"displayName": "jass-plugin",
	"description": "jass",
	"version": "0.0.4",
	"engines": {
		"vscode": "^1.50.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/NPOpenSource/jass-plugin.git"
	},
	"categories": [
		"Programming Languages"
	],
    "publisher":"826834678",
	"keywords": [
		"JASS",
		"vJASS",
		"Syntax",
		"Warcraft"
	],
	"activationEvents": [
		"onLanguage:vjass"
	],
	"main": "./extension.js",
	"contributes": {
		"grammars": [
			{
				"language": "vjass",
				"scopeName": "source.vjass",
				"path": "./syntaxes/vjass.json"
			}
		],
		"snippets": [
			{
				"language": "vjass",
				"path": "./snippets/vj_snippets.json"
			},
			{
				"language": "vjass",
				"path": "./APIParse/vj_snippets_function_cj.json"
			},
			{
				"language": "vjass",
				"path": "./APIParse/vj_snippets_variable_cj.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
